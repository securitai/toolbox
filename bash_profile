alias gsn='git status -uno'
alias gs='git status'
alias gd='git diff'
alias gdc='git diff --cached'
alias gau='git add -u'
alias gcq='git commit -qm'
alias gl='git log'
alias gb='git branch'
alias gba='git branch --all'
alias gk='gitk'
alias gka='gitk --all'

alias l='ls --color=always'
alias ll='ls -larth --color=always'

